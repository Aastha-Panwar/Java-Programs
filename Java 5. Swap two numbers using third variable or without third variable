public class Main
{
	public static void main(String[] args) {
	    int a=10, b=20;
	    System.out.println("Before value of a= "+ a+", value of b= "+b);
	    
	    // swap using third variable
	   // int c = a; 
	   // a= b;
	   // b=c;
	    
	    //swap without third variable, +-
	   // a=a+b;
	   // b=a-b;
	   // a=a-b;
	    
	    //swap without third variable, */
	    a=a*b;
	    a=a/b;
	    b=a/b;
	    
	    System.out.println("After value of a= "+ a+", value of b= "+b);	
	}
}
